{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/login",
      "destination": "/"
    },
    {
      "source": "/app",
      "destination": "/"
    },
    {
      "source": "/app/:path",
      "destination": "/"
    },
    {
      "source": "/api",
      "destination": "/api"
    },
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ]
}